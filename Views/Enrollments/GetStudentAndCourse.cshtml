@model IEnumerable<School.Models.Enrollment>

<h2>Alla elever som läser Programmering 1</h2>
<table class="table">
    <thead>
        <th>Kurs</th>
        <th>Elev</th>
        <th>Lärare</th>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Course.CourseName</td>
                <td>@item.Student.StudentName</td>
                <td>@item.Teacher.TeacherName</td>
            </tr>
        }
    </tbody>
</table>
